<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>  <!-- Must be true -->
  <headerType>gui</headerType>
  <jar>SampathCarDeco.jar</jar>
  <outfile>ShopApp.exe</outfile>
  <errTitle>Error</errTitle>
  <cmdLine>-cp SampathCarDeco.jar;lib/* Main</cmdLine>  <!-- Classpath here -->
  <chdir>.</chdir>
  <priority>normal</priority>
  <icon>resources/image/logo.ico</icon>
  <jre>
  <path>jre</path>
  <minVersion>1.8.0</minVersion>
  <bundledJre64Bit>true</bundledJre64Bit>
</jre>

</launch4jConfig>
